<?xml version='1.0' encoding='utf-8'?>
<widget id="com.LogicSquare.shaadi" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GooCon/</name>
    <description>
        A sample Apache Cordova application to illustrate Google Contact API.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-googleplus" spec="~4.0.6">
        <variable name="REVERSED_CLIENT_ID" value="457211481105-c56tbho46jm7c0hpdr7ssoiea1r3s3gv.apps.googleusercontent.com" />
    </plugin>
</widget>
